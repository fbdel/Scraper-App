

<div class="articles">
    {{#each articles}}
        <div class="bg-info text-white" id="articles">
            <h2>{{title}}</h2>
            <h3>{{location}}</h3>
            <h3>{{summary}}</h3>
            <a href ="{{link}}">{{link}}</a>
        </div>
        <div id="notes">
            <p>{{note}}</p>
        </div>


    {{/each}}
    {{#unless articles}}
    <h3 class="text-center">No Articles Scraped</h3>
    {{/unless}}
</div>